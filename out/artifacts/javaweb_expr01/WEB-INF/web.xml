<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>winterolympic.servlet.LoginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/winterolympic/LoginServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>register</servlet-name>
        <servlet-class>winterolympic.servlet.RegisterServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>register</servlet-name>
        <url-pattern>/winterolympic/RegisterServlet</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>home</servlet-name>
        <servlet-class>winterolympic.servlet.HomeServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>home</servlet-name>
        <url-pattern>/winterolympic/HomeServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>article</servlet-name>
        <servlet-class>winterolympic.servlet.ArticleServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>article</servlet-name>
        <url-pattern>/winterolympic/ArticleServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>cmpt</servlet-name>
        <servlet-class>winterolympic.servlet.CmptServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>cmpt</servlet-name>
        <url-pattern>/winterolympic/CmptServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>logout</servlet-name>
        <servlet-class>winterolympic.servlet.LogoutServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>logout</servlet-name>
        <url-pattern>/winterolympic/LogoutServlet</url-pattern>
    </servlet-mapping>
</web-app>